{
  "name": "@aureooms/js-fingertree",
  "description": "Finger Trees for JavaScript",
  "version": "5.0.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "timeout": "1m",
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-fingertree/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-itertools": "4.1.0",
    "@aureooms/js-measure": "1.0.3",
    "@aureooms/js-predicate": "2.0.2",
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/register": "7.11.5",
    "argparse": "2.0.1",
    "ava": "3.13.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "benchmark": "2.1.4",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fingertree": "0.1.7",
    "microtime": "3.0.0",
    "np": "6.5.0",
    "nyc": "15.1.0",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.7",
    "xo": "0.33.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "http://aureooms.github.io/js-fingertree",
  "keywords": [
    "data",
    "finger",
    "functional",
    "immutable",
    "javascript",
    "js",
    "persistent",
    "structure",
    "tree"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-fingertree"
  },
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "dev": "npm run lint -- --fix && npm run cover -- -- -st --fail-fast",
    "esdoc": "esdoc",
    "lint": "xo",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava",
    "travis": "npm run lint && npm run cover"
  },
  "sideEffects": false,
  "xo": {
    "prettier": true,
    "ignore": [
      "lib",
      "doc"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "camelcase": "off",
      "no-unused-vars": "off",
      "unicorn/filename-case": "off"
    },
    "overrides": [
      {
        "files": "_benchmark/*.js",
        "rules": {
          "import/no-unassigned-import": "off",
          "no-undef": "off"
        }
      },
      {
        "files": "_profile/*.js",
        "rules": {
          "import/no-unassigned-import": "off"
        }
      }
    ]
  }
}
