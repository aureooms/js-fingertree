{
  "name": "@aureooms/js-fingertree",
  "description": "Finger Trees for JavaScript",
  "version": "4.0.6",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "sources": [
      "src/**/*"
    ],
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-fingertree/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-itertools": "3.4.0",
    "@aureooms/js-measure": "1.0.3",
    "@aureooms/js-predicate": "2.0.2",
    "@babel/cli": "7.8.3",
    "@babel/core": "7.8.3",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/register": "7.8.3",
    "argparse": "1.0.10",
    "ava": "2.4.0",
    "benchmark": "2.1.4",
    "coveralls": "3.0.9",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fingertree": "0.1.7",
    "microtime": "3.0.0",
    "np": "5.2.1",
    "nyc": "14.1.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "http://aureooms.github.io/js-fingertree",
  "keywords": [
    "data",
    "finger",
    "functional",
    "immutable",
    "javascript",
    "js",
    "persistent",
    "structure",
    "tree"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-fingertree"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  }
}
